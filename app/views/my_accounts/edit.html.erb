<div style="text-align: right">

</div>
<div class="span-27 column outer-block align"
  <h5>Edit My Account</h5>
  <div class="inner-block account">
    <div style="padding:20px">
      <% form_for :update_my_account, @user,:url => { :action => :update }, :html => {:class=>"checkout-form"} do |f| %>
			  <%= f.error_messages %>
				<div class="inner-block">
					<fieldset>
					  <%= render :partial => "form", :object => f %>

							<div class="span-4 column">
							  <%= f.submit "Update", :type => "image", :src => "/images/update-admin.gif", :method => :put %>
							</div>


							<div class="span-3 column">
								<%= link_to image_tag("back-admin-large.gif", :width => "63", :height => "48", :alt => "" ), my_account_path %>
							</div>

					</fieldset>
				</div>
				<% end %>
    </div>
  </div>
</div>

