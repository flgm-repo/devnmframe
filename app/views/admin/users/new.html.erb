<% content_for :header_title do %>
  Register
<% end %>
<div class="span-33 clear content-area">
		<div class="clear"></div>
		<!-- Customize Options-->
		<div class="span-13 prepend-10">
			<div class="span-13 column outer-block">
				<% form_for [:admin,@user], :html => {:class=>"checkout-form"} do |f| %>
				  <%= f.error_messages %>
					<div class="inner-block">
						<fieldset>
							<%= render :partial => "form", :object => f %>


								<div class="span-4 column edit-margin">
								  <%= f.submit "Create", :type => "image", :src => "/images/create-user.gif" %>
								</div>


								<div class="span-3 column edit-margin">
								  <%= link_to image_tag("back-admin-large.gif", :width => "63", :height => "48", :alt => "" ), admin_users_path %>
								</div>

						</fieldset>
					</div>
					<% end %>
					<div class="clear"></div>
			</div>
		</div>
</div>

